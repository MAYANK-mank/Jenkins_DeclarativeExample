pipeline {
  agent any
  stages {
    stage('Start') {
      steps {echo "We are starting the pipeline and its jobs"}
    } 
    stage('Build') {
      steps {echo "Build is successful"}
    }
    stage('Test') {
      steps {echo "Test is complete"}
    }
    stage('Deploy') {
      steps {echo "Deploy is successful"}
    }
    stage('Release') {
      steps {echo "Release is successful"}
    }  
    stage('End') {
      steps {echo "All the jobs are successully completed."}
    }  
  }
}
